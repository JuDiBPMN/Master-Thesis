<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="BPMN Pipeline" exporterVersion="1.0">
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_customer" name="Customer" processRef="Process_customer"/>
    <bpmn:participant id="Participant_sales" name="Sales Department" processRef="Process_sales"/>
    <bpmn:participant id="Participant_logistics" name="Logistics Department" processRef="Process_logistics"/>
    <bpmn:messageFlow id="mf1" name="Invoice Sent" sourceRef="send_invoice" targetRef="gateway_sales"/>
    <bpmn:messageFlow id="MsgFlow_promoted_0" name="message" sourceRef="start_order" targetRef="check_availability"/>
    <bpmn:messageFlow id="MsgFlow_promoted_1" name="message" sourceRef="check_availability" targetRef="reject_order"/>
    <bpmn:messageFlow id="MsgFlow_promoted_2" name="message" sourceRef="send_invoice" targetRef="ship_goods"/>
  </bpmn:collaboration>
  <bpmn:process id="Process_customer" name="Customer" isExecutable="false">
    <bpmn:startEvent id="start_order" name="Order Placed"/>
  </bpmn:process>
  <bpmn:process id="Process_sales" name="Sales Department" isExecutable="false">
    <bpmn:endEvent id="end_order" name="Order Confirmed"/>
    <bpmn:task id="reject_order" name="Reject Order"/>
    <bpmn:task id="send_invoice" name="Send Invoice"/>
    <bpmn:exclusiveGateway id="gateway_sales" name="Approve Order" gatewayDirection="diverging"/>
    <bpmn:sequenceFlow id="flow3" sourceRef="reject_order" targetRef="send_invoice"/>
  </bpmn:process>
  <bpmn:process id="Process_logistics" name="Logistics Department" isExecutable="false">
    <bpmn:endEvent id="ship_goods_confirmed" name="Goods Shipped"/>
    <bpmn:task id="check_availability" name="Check Availability of Parts"/>
    <bpmn:task id="ship_goods" name="Ship Goods"/>
    <bpmn:parallelGateway id="gateway_logistics" name="Ship Goods" gatewayDirection="converging"/>
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">
      <bpmndi:BPMNShape id="Participant_customer_di" bpmnElement="Participant_customer" isHorizontal="true">
        <dc:Bounds x="10" y="20" width="900" height="200"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Participant_sales_di" bpmnElement="Participant_sales" isHorizontal="true">
        <dc:Bounds x="10" y="240" width="900" height="200"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Participant_logistics_di" bpmnElement="Participant_logistics" isHorizontal="true">
        <dc:Bounds x="10" y="460" width="900" height="200"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="start_order_di" bpmnElement="start_order">
        <dc:Bounds x="90" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="end_order_di" bpmnElement="end_order">
        <dc:Bounds x="90" y="322" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="reject_order_di" bpmnElement="reject_order">
        <dc:Bounds x="250" y="300" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="gateway_sales_di" bpmnElement="gateway_sales" isMarkerVisible="true">
        <dc:Bounds x="410" y="315" width="50" height="50"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="send_invoice_di" bpmnElement="send_invoice">
        <dc:Bounds x="570" y="300" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ship_goods_confirmed_di" bpmnElement="ship_goods_confirmed">
        <dc:Bounds x="90" y="542" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="check_availability_di" bpmnElement="check_availability">
        <dc:Bounds x="250" y="520" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ship_goods_di" bpmnElement="ship_goods">
        <dc:Bounds x="410" y="520" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="gateway_logistics_di" bpmnElement="gateway_logistics" isMarkerVisible="true">
        <dc:Bounds x="570" y="535" width="50" height="50"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="flow3_di" bpmnElement="flow3">
        <di:waypoint x="350" y="340"/>
        <di:waypoint x="570" y="340"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="mf1_di" bpmnElement="mf1">
        <di:waypoint x="620" y="300"/>
        <di:waypoint x="435" y="365"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="MsgFlow_promoted_0_di" bpmnElement="MsgFlow_promoted_0">
        <di:waypoint x="108" y="138"/>
        <di:waypoint x="300" y="520"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="MsgFlow_promoted_1_di" bpmnElement="MsgFlow_promoted_1">
        <di:waypoint x="300" y="520"/>
        <di:waypoint x="300" y="380"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="MsgFlow_promoted_2_di" bpmnElement="MsgFlow_promoted_2">
        <di:waypoint x="620" y="380"/>
        <di:waypoint x="460" y="520"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>